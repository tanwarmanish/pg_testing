    "build:prod": "ng build --prod --output-hashing=all --outputPath dist/prod",
    "build:ssr": "ng run IFS.Web:server:dev",
    "build:prodAotMaxSpace": "node --max_old_space_size=12288 ./node_modules/@angular/cli/bin/ng build --prod --aot --outputHashing=all --outputPath dist/prod",
